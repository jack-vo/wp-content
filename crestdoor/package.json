{
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prepend-file": "^1.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "symlink-divi": "ln -s ~/Desktop/workspace/wp-content/crestdoor/themes/Divi-child-1 /Applications/MAMP/htdocs/crestdoor/wp-content/themes/Divi-child-1",
    "remove-symlink-divi": "unlink /Applications/MAMP/htdocs/crestdoor/wp-content/themes/Divi-child-1",
    "symlink-bistro": "ln -s /Applications/MAMP/htdocs/crestdoor/wp-content/plugins/theme-customisations ~/Desktop/workspace/wp-content/crestdoor/themes/bistro-crestdoor/theme-customisations",
    "remove-symlink-bistro": "unlink ~/Desktop/workspace/wp-content/crestdoor/themes/bistro-crestdoor/theme-customisations",
    "webpack-watch": "webpack -d --watch",
    "webpack-build": "webpack -p",
    "divi-watch": "yarn webpack-watch --theme=Divi",
    "divi-build": "yarn webpack-build --theme=Divi",
    "watch": "yarn webpack-watch --config webpack.bistro.config.js",
    "build": "yarn webpack-build --config webpack.bistro.config.js"
  },
  "dependencies": {
    "swiper": "^4.5.0"
  }
}
